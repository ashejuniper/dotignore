{
  "name": "dotignore",
  "version": "0.1.2",
  "description": "ignorefile/includefile matching .gitignore spec",
  "main": "index.js",
  "bin": {
    "ignored": "bin/ignored"
  },
  "scripts": {
    "prepublishOnly": "safe-publish-latest",
    "prepublish": "not-in-publish || npm run prepublishOnly",
    "lint": "eslint --ext=js,mjs bin/* .",
    "pretest": "npm run lint",
    "tests-only": "nyc node test",
    "test": "npm run tests-only"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/bmeck/dotignore"
  },
  "keywords": [
    "ignore",
    "gitignore",
    "npmignore",
    "glob",
    "pattern"
  ],
  "author": {
    "name": "bradleymeck"
  },
  "license": "MIT",
  "dependencies": {
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^21.0.1",
    "eslint": "=8.8.0",
    "in-publish": "^2.0.1",
    "nyc": "^10.3.2",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.6.3"
  }
}
